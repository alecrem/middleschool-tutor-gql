{
  "name": "mcp-server",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "bin": {
    "middleschool-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
