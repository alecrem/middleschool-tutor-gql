{
  "buildCommand": "pnpm build",
  "outputDirectory": "apps/web/build/client",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "functions": {
    "apps/api/src/index.ts": {
      "runtime": "nodejs20.x"
    }
  }
}